{
  "name": "better-agent-terminal",
  "version": "1.25.1226094128",
  "description": "Windows terminal aggregator with multi-workspace support and Claude Code integration",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "compile": "vite build",
    "start": "electron .",
    "build": "vite build && node scripts/ensure-electron-cache.js && electron-builder",
    "build:release": "node scripts/build-version.js",
    "postinstall": "npm run setup:powershell && npm run setup:git && echo 'Run electron-rebuild manually if node-pty is needed'",
    "setup:powershell": "node scripts/setup-powershell.js",
    "setup:git": "node scripts/setup-git.js"
  },
  "dependencies": {
    "@lydell/node-pty": "^1.1.0",
    "@types/dompurify": "^3.0.5",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-unicode11": "^0.8.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/xterm": "^5.5.0",
    "basic-ftp": "^5.1.0",
    "better-sqlite3": "^12.5.0",
    "dompurify": "^3.3.1",
    "fflate": "^0.8.2",
    "highlight.js": "^11.11.1",
    "katex": "^0.16.27",
    "marked": "^17.0.1",
    "marked-katex-extension": "^5.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "ssh2-sftp-client": "^12.0.1",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.5"
  },
  "optionalDependencies": {
    "node-pty": "^0.10.1"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^9.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.0",
    "electron": "^28.3.3",
    "electron-builder": "^24.0.0",
    "png-to-ico": "^3.0.1",
    "sharp": "^0.34.5",
    "to-ico": "^1.1.5",
    "typescript": "^5.3.0",
    "unzipper": "^0.12.3",
    "vite": "^5.0.0",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "build": {
    "appId": "org.tonyq.better-agent-terminal",
    "productName": "BetterAgentTerminal",
    "executableName": "BetterAgentTerminal",
    "directories": {
      "output": "release"
    },
    "win": {
      "target": [
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ],
      "signAndEditExecutable": false,
      "icon": "assets/icon.ico"
    },
    "mac": {
      "target": {
        "target": "dmg",
        "arch": "universal"
      },
      "icon": "assets/icon.png",
      "category": "public.app-category.developer-tools"
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets/icon.png"
    },
    "forceCodeSigning": false,
    "npmRebuild": false,
    "electronDownload": {
      "cache": "packages/electron-cache"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "node_modules/**/*",
      "packages/**/*",
      "assets/icon.ico",
      "assets/icon.png"
    ]
  }
}
